{
  "name": "hasdemir/template",
  "type": "project",
  "description": "Hasdemir Framework.",
  "repositories": {
    "hasdemir/base": {
      "type": "path",
      "url": "../base",
      "options": {
        "symlink": true
      }
    }
  },
  "require": {
    "php": "^8.1",
    "respect/validation": "^2.2",
    "vlucas/phpdotenv": "^5.4",
    "hasdemir/base": "@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "minimum-stability": "stable"
}
